{"\u8b70\u6848\u5c46\u6b21\u5225":"\u8b70\u6848-\u7b2c1\u5c46\u7b2c6\u6b21","\u5927\u6703\u985e\u5225":"\u81e8\u6642\u5927\u6703","\u5be9\u67e5\u6703\u5225":"\u6c11\u653f","\u6848\u865f":"2","\u63d0\u6848\u985e\u5225":"\u8b70\u54e1\u63d0\u6848","\u4f86\u6587\u65e5\u671f":"","\u4f86\u6587\u5b57\u865f":"","\u63d0\u6848\u55ae\u4f4d\/\u4eba":["\u6797\u7f8e\u71d5"],"\u9023\u7f72\u4eba":["\u66fe\u57f9\u96c5\u3001\u8521\u7389\u679d\u3001\u90b1\u8389\u8389"],"\u4e3b\u65e8":"\u8acb\u6c11\u653f\u5c40\u5118\u901f\u56de\u8986\u5357\u5c71\u516c\u5893\u6574\u9ad4\u898f\u5283\u6210\u679c\u3002","\u8aaa\u660e":"\u5357\u5c71\u516c\u5893\u6574\u9ad4\u898f\u5283\u6848\u5df2\u7de8\u5217\u76f8\u95dc\u9810\u7b97\uff0c\u8acb\u5118\u901f\u8aaa\u660e\u76f8\u95dc\u9032\u5ea6\u8207\u6210\u679c\u3002","\u8fa6\u6cd5":"\u5982\u6848\u7531\u3002","\u5be9\u67e5\u610f\u898b":"\u7167\u6848\u901a\u904e\u3002","\u5927\u6703\u6c7a\u8b70":"\u7167\u5be9\u67e5\u610f\u898b\u901a\u904e\u3002","\u8fa6\u7406\u60c5\u5f62":"\u672c\u6848\u7d93\u5e02\u5e9c101\u5e743\u670813\u65e5\u5e9c\u6c11\u751f\u5b57\u7b2c1010181365\u865f\u51fd\u5fa9\uff1a\u63a8\u52d5\u8fa6\u7406\u91cc\u5357\u5c71\u516c\u5893\u6574\u9ad4\u898f\u5283\u6210\u679c\u4e00\u300199\u5e74\u5ea6\u8fa6\u7406\u63a8\u52d5\u5357\u5c71\u516c\u5893\u898f\u5283\uff0c\u5167\u5bb9\u5305\u62ec\u6e2c\u91cf\u3001\u9451\u754c\u3001\u5730\u4e0a\u7269\u6e2c\u7e6a\u3001\u5efa\u7acb\u5893\u7c4d\u6e05\u518a\u53ca\u6539\u5584\u74b0\u5883\u898f\u5283\u554f\u5377\u8abf\u67e5\u7b49\u4f5c\u696d\u3002(\u4e00)\u5357\u5c71\u516c\u5893\u5893\u7c4d\u3001\u5730\u7c4d\u6e05\u67e5\u4f5c\u696d\u5de5\u4f5c:\u57fa\u5730\u7bc4\u570d:\u570b\u6c11\u8def\u4ee5\u897f\u3001\u6c38\u5b89\u8857\u4ee5\u675f\u3001\u4e2d\u83ef\u5357\u8def\u4ee5\u5317(\u542b\u4e2d\u83ef\u5357\u8def\u4ee5\u5357\u5893\u5730)\u3001\u5927\u6210\u8def\u4ee5\u5357\u4e4b\u5357\u5c71\u516c\u5893\u7bc4\u570d\u4e4b\u571f\u5730\u3002\u5de5\u4f5c\u4e8b\u9805:(1)\u88fd\u4f5c\u5893\u7c4d\u6e05\u518a(\u5730\u4e0a\u6709\u5893\u4e3b\u4e4b\u58b3\u5893):\u5893\u7c4d\u6e05\u518a\u5305\u542b\u5893\u5740\u7de8\u865f\u3001\u5f80\u751f\u8005\u59d3\u540d\u3001\u7acb\u7891\u907a\u65cf\u59d3\u540d\u3001\u7acb\u7891\u5e74\u6708\u3001\u7956\u7c4d\u3001\u67e5\u4f30\u7b49\u7d1a\u3001\u7c21\u6613GPS\u5b9a\u4f4d\u5ea7\u6a19(\u8aa4\u5dee\u503c\u57285\u516c\u5c3a\u5167)\u7b49\u3002\u4e14\u5893\u7c4d\u6e05\u518a\u4ee5\u5730\u6bb5\u5340\u5206\u9020\u518a\uff0c\u5404\u5730\u6bb5\u5340\u57df\u5167\u4e26\u4f50\u4ee5\u5929\u7136\u5206\u754c\u7dda\u5340\u9694(\u5982\u9053\u8def\u5df7\u5f04\u3001\u6cb3\u5ddd)\u5206\u5225\u9020\u518a\u3002\u58b3\u5893\u6e05\u67e5\u7d50\u679c\u6574\u7406\u6210\u518a\u4e26\u5c07\u58b3\u5893\u4f4d\u7f6e\u9010\u4e00\u6a19\u793a\u65bc\u5730\u7c4d\u5957\u7e6a\u5716\u4e2d\u3002\u30082)\u5c07\u58b3\u5893\u4e88\u4ee5\u7de8\u865f\u4e26\u4ee5\u6578\u4f4d\u76f8\u6a5f\u5f69\u8272\u7167\u7247(\u81f3\u5c11\u542b\u5893\u7891\u8fd1  \u7167\u3001\u58b3\u5893\u5168\u7167\u53ca\u5893\u585a\u8fd1\u7167\u7b49)\uff0c\u5f59\u6574\u6210\u96fb\u5b50\u6a94\u3002(3)\u5893\u5340\u4f9d\u571f\u5730\u73fe\u6cc1\u505a\u5730\u7c4d\u5957\u7e6a\u4f5c\u696d\u53ca\u571f\u5730\u6e05\u518a\u7de8\u9020\u3002\u571f\u5730\u6e05\u518a\u5167\u5bb9\u5305\u62ec\u5730\u6bb5\u3001\u5730\u865f\u3001\u9762\u7a4d\u3001\u6240\u6709\u6b0a\u4eba\u6216\u7ba1\u7406\u4eba\u59d3\u540d\u3002(\u4e8c)\u5357\u5c71\u516c\u5893\u6539\u5584\u74b0\u5883\u898f\u5283\u554f\u5377\u8abf\u67e5:(\u672c\u6848\u4f9d\u64da\u539f\u81fa\u5357\u5e02\u8b70\u6703\u7b2c16\u5c46\u7b2c7\u6b21\u5b9a\u671f\u5927\u6703\u6797\u7f8e\u71d5\u8b70\u54e1\u5efa\u8b70\u6848\u8fa6\u7406)\u672c\u6848\u672c\u5e9c\u59d4\u7531\u7570\u8996\u884c\u92b7\u8abf\u67e5\u80a1\u4efd\u6709\u9650\u516c\u53f8\u8fa6\u7406\u554f\u5377\u8abf\u67e5:   \u672c\u6b21\u8abf\u67e5\uff0c\u5c0d\u65bc\u5e02\u653f\u5e9c\u898f\u5283\u5229\u7528\u589e\u5efa\u6216\u9077\u79fb\u7684\u65b9\u5f0f\u89e3\u6c7a\u5bb9\u91cf  \u98fd\u548c\u7684\u554f\u984c\uff0c\u6709\u516d\u6210\u516d(66\uff0e0%))\u8868\u793a\u8d0a\u6210\uff0c\u800c\u5c0d\u65bc\u589e\u5efa\u6216\u9077\u79fb\u4f7f\u7528\u7684\u65b9\u5f0f\uff0c\u6709\u516d\u6210\u516b(68\uff0e2%)\u8868\u793a\u652f\u6301\u5e02\u5e9c\u5229\u7528\u5357\u5c71\u516c\u5893\u7528\u5730\u4f86\u898f\u5283\u53e6\u4e00\u5ea7\u6baf\u5100\u9928\uff0c\u6709\u8fd1\u4e03\u54b8\u4e8c(71\uff0e7)\u8868\u793a\u652f\u6301\u5c07\u73fe\u5740\u6baf\u5100\u9928\u9077\u79fb\u5165\u5357\u5c71\uff0c\u4e26\u898f\u5283\u6210\u6baf\u846c\u5c08\u7528\u5340\u300d\uff0c\u91dd\u5c0d\u5c11\u6578\u4e0d\u652f\u6301\u7684\u53d7\u8a2a\u8005\uff0c\u672c\u8abf\u67e5\u9032\u4e00\u6b65\u8a62\u554f\u9019\u4e9b\u53d7\u8a2a\u8005\u4e0d\u652f\u6301\u7684\u539f\u56e0\uff0c\u5927\u90e8\u5206\u7684\u53d7\u8a2a\u8005\u7686\u8868\u793a\u4f4f\u5bb6\u591a\uff0c\u4e0d\u9069\u5408\u518d\u589e\u5efa\u6baf\u5100\u9928\u6216\u662f\u898f\u5283\u6210\u6baf\u846c\u5c08\u7528\u5340(36\uff0e8%)\u3002\u5728\u9077\u79fb\u7684\u898f\u756b\u53ca\u76f8\u95dc\u65b9\u6848\u4e0a\uff0c\u6709\u516d\u6210\u516d(65\uff0e8%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u82e5\u653f\u5e9c\u88dc\u52a9\u58b3\u5893\u9077\u79fb\u734e\u52f5\u91d1\u9858\u610f\u914d\u5408\u9077\u79fb\uff0c\u6709\u516b\u6210\u516b(88\uff0e1%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u652f\u6301\u5e02\u5e9c\u898f\u5283\u9032\u884c\u9053\u8def\u95e2\u5efa\u5de5\u7a0b\u4ee5\u6539\u5584\u5e02\u6c11\u524d\u5f80\u6383\u5893\u796d\u7956\u7684\u4fbf\u5229\u5ea6\uff0c\u6709\u516b\u6210\u4e09(83\uff0e0%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u652f\u6301\u5728\u5357\u5c71\u516c\u5893\u56db\u5468\uff0c\u898f\u5283\u505c\u8eca\u5834\uff0c\u589e\u52a0\u5e02\u6c11\u505c\u8eca\u7a7a\u9593\uff0c\u6709\u8fd1\u4e03\u6210\u4e5d(78\uff0e9%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u82e5\u63a8\u52d5\u66f4\u65b0\u5357\u5c71\u516c\u5893\u8d0a\u6210\u5c07\u539f\u6709\u90e8\u4efd\u5893\u5730\u6539\u5efa\u6210\u7d0d\u9aa8\u5854\uff0c\u91dd\u5c0d\u5c11\u6578\u4e0d\u652f\u6301\u7684\u53d7\u8a2a\u8005\uff0c\u672c\u8abf\u67e5\u9032\u4e00\u6b65\u8a62\u554f\u9019\u4e9b\u53d7\u8a2a\u8005\u4e0d\u8d0a\u6210\u7684\u539f\u56e0\uff0c\u4ee5\u6703\u300c\u5f71\u97ff\u5230\u539f\u6709\u516c\u5893\u5bb6\u5c6c\u7684\u6b0a\u76ca\uff0c\u6703\u88ab\u8981\u6c42\u8981\u628a\u5bb6\u4eba\u5893\u5730\u9077\u846c\u5230\u5225\u8655\u300d\u6240\u5360\u7684\u6bd4\u4f8b\u6700\u9ad8(44\uff0e6%)\u3002\u8abf\u67e5\u767c\u73fe\u7406\u60f3\u7684\u5893\u5712\u53ca\u7d0d\u9aa8\u5854\u9808\u5177\u5099\u7684\u689d\u4ef6\uff0c\u5728\u5893\u5712\u7684\u90e8\u5206\uff0c\u8868\u793a\u4ea4\u901a\u4fbf\u5229\u7684\u53d7\u8a2a\u8005\u5360\u516d\u6210\u56db(64\uff0e4%)\u7684\u6bd4\u4f8b\u6700\u591a\uff0c\u5176\u6b21\u662f\u6574\u6f54\u7f8e\u89c0(58\uff0e3%)\u3001\u50f9\u683c\u4fbf\u5b9c(55\uff0e2%)\u3001\u6709\u5c08\u4eba\u796d\u62dc\u8ca0\u8cac\u7ba1\u7406(50\uff0e8%);\u5728\u9748\u9aa8\u5854\u7684\u90e8\u5206\uff0c\u8868\u793a\u4ea4\u901a\u4fbf\u5229\u7684\u53d7\u8a2a\u8005\u5360\u516d\u6210\u56db(64\uff0e4%)\u7684\u6bd4\u4f8b\u6700\u591a\uff0c\u5176\u6b21\u662f\u6574\u6f54\u7f8e\u89c0(58\uff0e3%)\u3001\u50f9\u683c\u4fbf\u5b9c(55\uff0e2%  \u300b\u3001\u5c08\u4eba\u796d\u62dc\u8ca0\u8cac\u7ba1\u7406(50\uff0e8%)\uff0c\u4e8c\u8005\u56de\u7b54\u7684\u689d\u4ef6\u76f8\u8ddd\u4e0d\u9060\u3002\u6700\u5f8c\uff0c\u5c0d\u65bc\u672a\u4f86\u5e0c\u671b\u516c\u5893\u63a8\u52d5\u6539\u5584\u7684\u90e8\u5206\uff0c\u6709\u4e94\u6210(50\uff0e2%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u540c\u610f\u672a\u4f86\u5c07\u5357\u5c71\u516c\u5893\u59d4\u8a17\u6c11\u9593\u7ba1\u7406\u7dad\u8b77;\u6709\u4e03\u6210\u516b(78\uff0e3%)\u7684\u53d7\u8a2a\u8005\u8868\u793a\u540c\u610f\u672a\u4f86\u653f\u5e9c\u6bcf\u5e74\u512a\u5148\u7de8\u5217\u7d93\u8cbb\u53ca\u4eba\u529b\u4f86\u6539\u5584\u4e26\u7dad\u8b77\u5357\u5c71\u516c\u5893;\u4e26\u8a8d\u70ba\u8981\u5982\u4f55\u8b93\u6c11\u773e\u53c3\u8207\u4e26\u4e86\u89e3\u5357\u5c71\u516c\u5893\u7684\u8a2d\u65bd\u914d\u7f6e\uff0c\u63a1\u53d6\u300c\u7531\u653f\u5e9c\u8fa6\u7406\u807d\u8b49\u6703\u300d\u7684\u6bd4\u4f8b\u6700\u591a\uff0c\u5360\u4e09\u6210\u516d(36\uff0e2%)\uff0c\u800c\u91dd\u5c0d\u6c11\u773e\u958b\u653e\u5f0f\u7684\u5efa\u8b70\uff0c\u670997\u4f4d\u8a8d\u70ba\u5e0c\u671b\u52a0\u5f37\u6539\u5584\u7684\u5730\u65b9\u3002\u6700\u4e3b\u8981\u5e0c\u671b\u898f\u5283\u6642\u80fd\u53c3\u8003\u591a\u65b9\u610f\u898b\uff0c\u5176\u6b21\u70ba\u96e2\u5e02\u5340\u9060\u4e00\u9ede\u3001\u50f9\u683c\u8abf\u964d\u5408\u7406\u5316\u3001\u74b0\u5883\u8981\u6574\u6f54\u7f8e\u89c0\u3002\u4e8c\u3001100\u5e74\u5ea6\u7de8\u5217\u5357\u5c71\u516c\u5893\u898f\u5283\u6848\u7d93\u8cbb530\u842c\u5143\u8fa6\u7406\u6e2c\u91cf\u9451\u754c\u7528\u5730\u5927\u539f\u5247\u898f\u5283(\u5167\u5bb9\u5305\u542b\u5730\u4e0a\u7269\u6e2c\u7e6a\u53ca\u8a2d\u65bd\u898f\u5283\u6848\u7b49\u5927\u539f\u5247\u898f\u5283\u76f8\u95dc\u884c\u653f\u5de5\u4f5c)\u53ca\u516c\u51526\uff0e7\uff0e8\uff0e19\u5730\u4e0a\u7269\u67e5\u4f30\u53ca\u62c6\u9077\u88dc\u511f\u4f5c\u696d\u7d93\u8cbb4\u4edf\u842c\u5143\u3002\u8aaa\u660e: \u5357\u5c71\u516c\u5893\u5df2\u5168\u9762\u7981\u846c\uff0c\u5176\u7528\u5730\u76ee\u524d\u5df2\u7121\u63d0\u4f9b\u505a\u70ba\u516c\u5893\u7528\u5730\u4e4b\u9700\u6c42\uff0c\u570b\u6c11\u8def\u897f\u5074(\u5927\u6210\u8def\u81f3\u4e2d\u83ef\u5357\u8def\u53e3)\u571f\u5730\uff0c\u539f\u53f0\u5357\u5e02\u653f\u5e9c98.7\uff0e21\u5357\u5e02\u6c11\u5b97\u5b57\u7b2c09810525370\u865f\u51fd\u8acb\u90fd\u5e02\u767c\u5c55\u5c40\u8fa6\u7406\u5f8c\u7e8c\u90fd\u5e02\u8a08\u756b\u4f5c\u696d\u4e8b\u5b9c\u3002  \u5e02\u5e9c\u5c0d\u65bc\u5357\u5c71\u516c\u5893\u5927\u539f\u5247\u898f\u5283\u76f8\u95dc\u884c\u653f\u5de5\u4f5c\uff0c\u4f9d\u5357\u5c71\u516c\u5893\u6539\u5584\u74b0\u5883\u898f\u5283\u554f\u5377\u8abf\u67e5\uff0c\u671d\u78ba\u4fdd\u6c11\u773e\u751f\u6d3b\u54c1\u8cea\u63d0\u51fa\u672a\u4f86\u898f\u5283\u3001\u7a4d\u6975\u8207\u6c11\u6e9d\u901a\u64ec\u5b9a\u5b8c\u5584\u7684\u9077\u79fb\u653f\u7b56\u3001\u8003\u616e\u52d5\u7dda\u5b89\u6392\u589e\u8a2d\u505c\u8eca\u5340\u4f4d\u3001\u63d0\u9ad8\u846c\u5100\u670d\u52d9\u54c1\u8cea\u52a0\u5f37\u56db\u5468\u74b0\u5883\u7f8e\u5316\u7b49\u5efa\u8b70\u898f\u5283\u3002*\u5de5\u52d9\u5c40100.11\uff0e8\uff0e31\u5357\u5e02\u5de5\u516c\u5712\u4e00\u5b57\u7b2c1000675705\u865f\u51fd\uff0c\u6709\u95dc\u672c\u5e02\u5357\u5c71\u516c\u5893\u6c38\u6210\u8def\u8207\u5357\u5c71\u516c\u5893\u9593\u7da0\u5e36\u9077\u846c\u571f\u5730\u6d3b\u5316\u8a08\u756b(\u516c\u51526\uff0e7\uff0c8\uff0e19)\u5730\u4e0a\u7269\u67e5\u4f30\u53ca\u62c6\u9077\u88dc\u511f\u4f5c\u696d\uff0c\u8acb\u6baf\u846c\u7ba1\u7406\u6240\u4ee3\u8fa6\u58b3\u5893\u67e5\u4f30\u53ca\u9077\u846c\u88dc\u511f\u4f5c\u696d\uff0c\u53e6\u672c\u6848\u6240\u7de8\u5217\u4e4b4\u4edf\u842c\u5143\u5730\u4e0a\u7269\u88dc\u511f\u8cbb\u9810\u7b97(\u542b\u9451\u754c\u8cbb\u7528)\u9810\u7b97\uff0c\u7531\u5de5\u52d9\u5c401OO\u5e74\u5ea6\u516c\u5171\u5de5\u7a0b\u8655--\u516c\u5171\u5de5\u7a0b\u5efa\u7bc9\u53ca\u8a2d\u5099\u4e00\u516c\u5712\u53ca\u8def\u71c8\u8a2d\u65bd\u4e00\u8a2d\u5099\u53ca\u6295\u8cc7--\u571f\u5730\u7d93\u8cbb\u9805\u4e0b\u652f\u61c9\uff0c\u672c\u6848\u76ee\u524d\u7531\u6baf\u846c\u6240\u8fa6\u7406\u516c\u544a\u3001\u58b3\u5893\u67e5\u4f30\u53ca\u9077\u846c\u88dc\u511f\u4f5c\u696d\u4e2d\u3002*\u6c11\u653f\u5c40100\uff0e11\uff0e9\u5357\u5e02\u6c11\u751f\u5b57\u7b2c1000875530\u865f\u51fd\uff0c\u6aa2\u9001\u300c\u5357\u5c71\u516c\u5893\u6574\u9ad4\u898f\u5283\u7db1\u8981\u300d\u66a8\u5de5\u52d9\u5c40\u3001\u5730\u653f\u5c40\u3001\u6c34\u5229\u5c40\u3001\u90fd\u5e02\u767c\u5c55\u5c40\u7b49\u76f8\u95dc\u5c40\u5ba4\u8cc7\u6599\uff0c\u51fd\u8acb\u6baf\u846c\u7ba1\u7406\u6240\u4f9d\u63a1\u8cfc\u6cd5\u8fa6\u7406\u898f\u5283\u516c\u53f8\u8a55\u9078\u4e8b\u5b9c;\u672c\u6848\u76ee\u524d\u7531\u6baf\u846c\u6240\u8fa6\u7406\u516c\u958b\u62db\u6a19\u8a55\u9078\u3002","\u6c7a\u8b70\u65e5\u671f":"2012\/2\/22","\u767c\u6587\u65e5\u671f":"2012\/2\/29","\u767c\u6587\u5b57\u865f":"\u5357\u8b70\u6c11\u653f\u5b57\u7b2c1010010164\u865f","\u8b70\u6703\u9644\u4ef6\u4e00":""}