{"\u8b70\u6848\u5c46\u6b21\u5225":"\u8b70\u6848-\u7b2c1\u5c46\u7b2c3\u6b21","\u5927\u6703\u985e\u5225":"\u5b9a\u671f\u5927\u6703","\u5be9\u67e5\u6703\u5225":"\u6c11\u653f","\u6848\u865f":"6","\u63d0\u6848\u985e\u5225":"\u8b70\u54e1\u63d0\u6848","\u4f86\u6587\u65e5\u671f":"","\u4f86\u6587\u5b57\u865f":"","\u63d0\u6848\u55ae\u4f4d\/\u4eba":["\u90ed\u570b\u6587"],"\u9023\u7f72\u4eba":["\u90b1\u8389\u8389\u3001\u8cf4\u60e0\u54e1"],"\u4e3b\u65e8":"\u5efa\u8acb\u5e02\u5e9c\u6e05\u67e5\u5168\u5e02\u9053\u8def\u540d\u7a31\uff0c\u4e86\u89e3\u5168\u5e02\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u3001\u8def\u540d\u540c\u5340\u537b\u540c\u97f3\u7570\u5b57\u7684\u72c0\u6cc1\uff0c\u4e26\u4f5c\u51fa\u660e\u78ba\u7684\u7ba1\u7406\u8a08\u756b\uff0c\u9810\u5b9a\u65bd\u4f5c\u7684\u6642\u7a0b\uff0c\u9700\u91cd\u65b0\u547d\u540d\u7684\u9053\u8def\u53ef\u8207\u7576\u5730\u6b77\u53f2\u9023\u7d50\uff0c\u5728\u5730\u5316\u540d\u7a31\u53ef\u6539\u5584\u9019\u7a2e\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u73fe\u8c61\u3002","\u8aaa\u660e":"\u7e23\u5e02\u5408\u4f75\u5f8c\uff0c\u5728\u7e23\u5e02\u4ea4\u754c\u5b58\u6709\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u554f\u984c\uff0c\u5982\u540c\u4e00\u689d\u8def\u5728\u6771\u5340\u7a31\u70ba\u88d5\u7fa9\u8def\u904e\u4e00\u500b\u8def\u53e3\u5728\u4ec1\u5fb7\u5340\u7a31\u4f5c\u6587\u5fb7\u8def\uff0c\u5317\u5340\u897f\u9580\u8def\u56db\u6bb5\u5165\u6c38\u5eb7\u5340\u7a31\u5c0f\u5317\u8def\uff0c\u800c\u81e8\u8fd1\u7684\u5317\u5340\u897f\u9580\u8def\u56db\u6bb5\u3001\u4e09\u6bb5\u4ea4\u754c\u8655\u4ea6\u6709\u4e00\u689d\u5c0f\u5317\u8def\uff0c\u9020\u6210\u9130\u8fd1\u5340\u57df\u4e00\u540d\u591a\u8def\uff0c\u65b0\u5316\u5340\u4fe1\u7fa9\u8857\u904e\u4e00\u8def\u53e3\u6210\u4e86\u4fe1\u7fa9\u8def\uff0c\u4e00\u8def\u591a\u540d\uff0c\u4e14\u50c5\u8def\u3001\u8857\u4e0d\u540c\uff0c\u589e\u6dfb\u6c11\u773e\u7528\u8def\u56f0\u64fe\u3002","\u8fa6\u6cd5":"\u4e00\u3001\u5168\u9762\u666e\u67e5\uff0c\u4e86\u89e3\u5168\u5e02\u591a\u5c11\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u3001\u8def\u540d\u540c\u4e00\u5340\u537b\u540c\u97f3\u7570\u5b57\u7684\u72c0\u6cc1\uff0c\u4f5c\u51fa\u660e\u78ba\u7684\u7ba1\u7406\u8a08\u5283\uff0c\u9810\u5b9a\u65bd\u4f5c\u7684\u6642\u7a0b\u3002\u4e8c\u3001\u65bc\u5404\u884c\u653f\u5340\u57df\u7bc4\u570d\u78ba\u5b9a\u5f8c\uff0c\u9032\u884c\u9053\u8def\u547d\u540d\uff0c\u4e26\u5c0d\u65bc\u8981\u66f4\u6539\u6216\u6574\u5408\u7684\u8def\u540d\uff0c\u8a02\u51fa\uff62\u843d\u65e5\u6642\u9593\uff63\uff0c\u914d\u5408103\u5e74\u63db\u767c\u9580\u724c\u8207\u8eab\u5206\u8b49\u6642\u4e00\u540c\u6574\u4f75\u66f4\u63db\uff0c\u907f\u514d\u9020\u6210\u6c11\u773e\u4e0d\u4fbf\u3002\u4e09\u3001\u9700\u91cd\u65b0\u547d\u540d\u7684\u9053\u8def\u53ef\u8207\u7576\u5730\u6b77\u53f2\u9023\u7d50\uff0c\u5728\u5730\u5316\u540d\u7a31\u53ef\u6539\u5584\u9019\u7a2e\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u73fe\u8c61\u3002","\u5be9\u67e5\u610f\u898b":"\u51fd\u9001\u5e02\u5e9c\u7814\u8fa6\u3002","\u5927\u6703\u6c7a\u8b70":"\u7167\u5be9\u67e5\u610f\u898b\u901a\u904e\u3002","\u8fa6\u7406\u60c5\u5f62":"\u4f9d\u64da\u81fa\u5357\u5e02\u653f\u5e9c101\u5e746\u670819\u65e5\u5e9c\u6c11\u5b57\u7b2c1010494997\u865f\u51fd\u5fa9\uff1a\u4e00\u3001\u672c\u5e9c\u6c11\u653f\u5c40\u5df2\u5168\u9762\u6e05\u67e537\u5340\u9053\u8def\uff0c\u5efa\u7acb\u300c\u81fa\u5357\u5e02\u9053\u8def\u540d\u7a31\u8cc7\u6599\u5eab\u300d\uff0c\u81fa\u5357\u5e02\u9053\u8def\uff08\u542b\u5730\u540d\uff09\u5171\u8a083,000\u689d\uff0c\u76f8\u540c\u540d\u7a31\u4e4b\u9053\u8def\u5171\u8a08260\u9805801\u689d\u9053\u8def\uff0c\u76f8\u540c\u540d\u7a31\u9053\u8def\u8a2d\u7c4d\u6236\u6578\u8207\u4eba\u6578\u5171\u8a0821\u842c4839\u6236\uff0c60\u842c2622\u4eba\uff0c\u5f71\u97ff\u5c64\u9762\u751a\u9245\u3002\u4e8c\u3001\u70ba\u907f\u514d\u6d6a\u8cbb\u516c\u5e11\u53ca\u8003\u91cf\u6c11\u773e\u91cd\u8907\u63db\u767c\u8b49\u4ef6\u4e0d\u4fbf\uff0c\u5404\u5340\u6236\u653f\u4e8b\u52d9\u5c07\u4fdf\u884c\u653f\u5340\u57df\u91cd\u5283\u78ba\u5b9a\u5f8c\uff0c\u518d\u7814\u8b70\u76f8\u540c\u540d\u7a31\u9053\u8def\u66f4\u540d\u4e8b\u5b9c\uff0c\u4e26\u7d71\u4e00\u514d\u8cbb\u66f4\u63db\u9580\u724c\u3001\u63db\u767c\u570b\u6c11\u8eab\u5206\u8b49\u53ca\u6539\u8a3b\u6236\u53e3\u540d\u7c3f\uff0c\u76e1\u91cf\u8b93\u6c11\u773e\u7684\u4e0d\u4fbf\u964d\u5230\u6700\u4f4e\u3002\u4e09\u3001\u6aa2\u9001\u300c\u81fa\u5357\u5e02\u76f8\u540c\u540d\u7a31\u9053\u8def\u7ba1\u7406\u8a08\u756b\u300d1\u4efd\u4f9b\u53c3\u3002\u9644\u4ef6\uff1a\u81fa\u5357\u5e02\u76f8\u540c\u540d\u7a31\u9053\u8def\u7ba1\u7406\u8a08\u756b\u58f9\u3001\u524d\u8a00\uff1a\u70ba\u56e0\u61c9\u884c\u653f\u5340\u57df\u6574\u4f75\uff0c\u6709\u6548\u7ba1\u7406\u76f8\u540c\u540d\u7a31\u9053\u8def\uff0c\u6539\u5584\u4e00\u8def\u591a\u540d\u3001\u4e00\u540d\u591a\u8def\u73fe\u8c61\u66a8\u6e1b\u5c11\u6c11\u773e\u91cd\u8907\u63db\u767c\u8b49\u4ef6\u4e0d\u4fbf\uff0c\u64ec\u8a02\u76f8\u540c\u9053\u8def\u91cd\u65b0\u547d\u540d\u77ed\u3001\u4e2d\u3001\u9577\u7a0b\u57f7\u884c\u7b56\u7565\uff1a\u8cb3\u3001\u77ed\u671f\u57f7\u884c\u7b56\u7565\uff1a\u4e00\u3001\u70ba\u7a69\u5b9a\u6c11\u5fc3\u3001\u907f\u514d\u6c11\u773e\u6050\u614c\uff0c\u6539\u5236\u521d\u671f\uff0c\u9053\u8def\u66ab\u6642\u7dad\u6301\u539f\u540d\u7a31\uff0c\u4e26\u52a0\u8a3b\u884c\u653f\u5340\u540d\uff0c\u4ee5\u5229\u8fa8\u8b58\uff0c\u6301\u7e8c\u5ba3\u5c0e\u6c11\u773e\u914d\u5408\u57f7\u884c\u3002\u4e8c\u3001\u5efa\u7acb\u9053\u8def\u540d\u7a31\u8cc7\u6599\u5eab\uff1a \uff08\u4e00\uff09\u81fa\u5357\u5e02\u9053\u8def\uff08\u542b\u5730\u540d\uff09\u5171\u8a083,000\u689d\u3002\uff08\u4e8c\uff09\u76f8\u540c\u8def\u540d\uff1a260\u9805801\u689d\u76f8\u540c\u540d\u7a31\u4e4b\u9053\u8def\u3002\uff08\u4e09\uff09\u4e2d\u5c71\u8def19\u7b46\u3001\u4e2d\u6b63\u8def15\u7b46\u3001\u4ec1\u611b\u885711\u689d\u5c45\u591a\u3002\uff08\u56db\uff09\u540c\u5340\u540c\u97f3\u7570\u5b57\u4e4b\u9053\u8def\u5171\u8a083\u7d446\u689d\uff1a1. \u65b0\u5e02\u5340\uff1a\u4e2d\u5fc3\u8def\u3001\u4e2d\u8208\u8857\u30012.\u6c38\u5eb7\u5340\uff1a\u632f\u8208\u8def\u3001\u6b63\u65b0\u8857\u30013. \u6c38\u5eb7\u5340\uff1a\u6c38\u5fe0\u8def\u3001\u6c38\u4e2d\u8857\u3002\uff08\u4e94\uff09\u76f8\u540c\u540d\u7a31\u9053\u8def\u8a2d\u7c4d\u6236\u6578\u8207\u4eba\u6578:\u6236\u657821\u842c4,839\u6236\uff0c60\u842c2,622\u4eba\u3002\uff08\u516d\uff09\u5404\u5340\u6236\u653f\u4e8b\u52d9\u6240\u65bc\u6539\u5236\u5f8c\u65b0\u95e2\u9053\u8def\u547d\u540d\u6216\u9053\u8def\u66f4\u540d\u540d\u7a31\uff0c\u61c9\u5148\u67e5\u8a62\u9053\u8def\u8cc7\u6599\u5eab\uff0c\u907f\u514d\u91cd\u8907\u547d\u540d\u3002\uff08\u4e03\uff09\u65b0\u95e2\u9053\u8def\u547d\u540d\u53ca\u66f4\u540d\u540d\u7a31\u9001\u5e9c\u6838\u5b9a\u5f8c\uff0c\u7531\u6c11\u653f\u5c40\u66f4\u65b0\u9053\u8def\u540d\u7a31\u8cc7\u6599\u5eab\uff0c\u4f9b\u5404\u5340\u6236\u653f\u4e8b\u52d9\u6240\u67e5\u8a62\u3002\u4e09\u3001\u6301\u7e8c\u8490\u96c6\u6c11\u773e\u53cd\u6620\u3002\u56db\u3001\u5236\u5b9a\u672c\u5e02\u9053\u8def\u547d\u540d\u53ca\u9580\u724c\u7de8\u91d8\u81ea\u6cbb\u689d\u4f8b\uff0c\u4ee5\u70ba\u8fa6\u7406\u9053\u8def\u547d\u540d\u53ca\u9580\u724c\u7de8\u91d8\u4e4b\u4f9d\u64da\u3002\u4e94\u3001\u70ba\u6e1b\u5c11\u6c11\u773e\u91cd\u8907\u63db\u767c\u8b49\u4ef6\u4e0d\u4fbf\uff0c\u4f9d\u300c\u81fa\u5357\u5e02\u9053\u8def\u547d\u540d\u53ca\u9580\u724c\u7de8\u91d8\u81ea\u6cbb\u689d\u4f8b\u300d\u7b2c7\u689d\u7b2c2\u9805\u898f\u5b9a\uff0c\u7b26\u5408\u9053\u8def\u547d\u540d\u5206\u985e\u539f\u5247\uff0c\u6709\u5fc5\u8981\u5f97\u66f4\u6539\u3002\u516d\u3001\u65b0\u95e2\u9053\u8def\u4e4b\u547d\u540d\u539f\u5247\u4ee5\u5f70\u986f\u672c\u5e02\u5730\u65b9\u7279\u8272\u3001\u98a8\u4fd7\u7fd2\u6163\u3001\u6587\u5316\u53e4\u90fd\u80cc\u666f\u70ba\u4e3b\uff0c\u914d\u5408\u9053\u8def\u8d77\u8fc4\u76ee\u7684\u5730\u3001\u884c\u653f\u5340\u3001\u65b9\u5411\u53ca\u6578\u5b57\u7b49\u547d\u540d\u7cfb\u7d71\uff0c\u907f\u514d\u4f7f\u7528\u7f55\u898b\u5b57\u53ca\u4e0d\u96c5\u5b57\u773c\u3002\u53c3\u3001\u4e2d\u671f\u57f7\u884c\u7b56\u7565\uff1a\u4e00\u3001\u90ca\u5340\u672a\u547d\u540d\u4e4b\u5340\u57df\uff0c\u914d\u5408\u90fd\u5e02\u8a08\u756b\uff0c\u4f75\u958b\u95e2\u65b0\u9053\u8def\u547d\u540d\uff0c\u539f\u5730\u540d\u53ef\u4fdd\u7559\u70ba\u9053\u8def\u540d\u7a31\u3002\u4e8c\u3001\u9053\u8def\u547d\u540d\u4e8b\u6d89\u884c\u653f\u5340\u57df\u6574\u4f75\u3001\u90fd\u5e02\u8a08\u756b\u3001\u9053\u8def\u958b\u95e2\u7b49\u5c08\u696d\u55ae\u4f4d\uff0c\u7531\u6c11\u653f\u5c40\u6703\u540c\u4ea4\u901a\u5c40\u3001\u5de5\u52d9\u5c40\u3001\u90fd\u767c\u5c40\u7b49\u6a5f\u95dc\uff0c\u4f9d\u64da\u672c\u5e02\u5340\u57df\u7279\u6027\u53ca\u4ea4\u901a\u7db2\u7d61\u767c\u5c55\u6574\u9ad4\u898f\u5283\u9053\u8def\u91cd\u65b0\u547d\u540d\u4e8b\u5b9c\u3002\u8086\u3001\u9577\u671f\u57f7\u884c\u7b56\u7565\uff1a\u4e00\u3001\u914d\u5408\u884c\u653f\u5340\u57df\u8abf\u6574\u671f\u7a0b\u53ca\u7bc4\u570d\uff0c\u78ba\u8a8d\u9808\u91cd\u65b0\u547d\u540d\u4e4b\u9053\u8def\u3002\u4e8c\u3001\u900f\u904e\u91cc\u6c11\u5ea7\u8ac7\u6703\u53ca\u57fa\u5c64\u6703\u8b70\uff0c\u7814\u5546\u9053\u8def\u91cd\u65b0\u547d\u540d\u5099\u9078\u65b9\u6848\uff0c\u532f\u96c6\u5730\u65b9\u5171\u8b58\uff0c\u516c\u958b\u8868\u6c7a\u51fa\u6700\u4f73\u65b9\u6848\uff0c\u5831\u8acb\u5e02\u5e9c\u6838\u5b9a\u3002\u4e09\u3001\u9053\u8def\u91cd\u65b0\u547d\u540d\u4e4b\u5340\u57df\uff0c\u8fa6\u7406\u9580\u724c\u6574\u7de8\uff0c\u5982\u6709\u9580\u724c\u865f\u78bc\u9806\u5e8f\u91cd\u8907\u51cc\u4e82\u6216\u4e0d\u5408\u898f\u5b9a\u8005\uff0c\u4e00\u4f75\u6574\u7de8\u3002\u56db\u3001\u6c11\u773e\u56e0\u5340\u57df\u6574\u4f75\u6216\u9053\u8def\u91cd\u65b0\u547d\u540d\uff0c\u6236\u653f\u4e8b\u52d9\u6240\u514d\u8cbb\u7d71\u4e00\u66f4\u63db\u9580\u724c\u3001\u8eab\u5206\u8b49\u3001\u6236\u53e3\u540d\u7c3f\uff0c\u4e26\u914d\u5408\u91cc\u9130\u9577\u81f3\u8f44\u5340\u63db\u767c\u4e0a\u63ed\u8b49\u4ef6\u3002","\u6c7a\u8b70\u65e5\u671f":"2012\/6\/6","\u767c\u6587\u65e5\u671f":"2012\/6\/13","\u767c\u6587\u5b57\u865f":"\u5357\u8b70\u6c11\u653f\u5b57\u7b2c1010010560\u865f","\u8b70\u6703\u9644\u4ef6\u4e00":""}