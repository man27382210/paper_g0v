901a\u904e(\u4fee\u6b63\u60c5\u5f62\u8a73\u5982\u5404\u59d4\u54e1\u6703\u9810\u7b97\u522a\u6e1b\u660e\u7d30\u8868)\u3002\u4e09\u3001\u9644\u5e36\u6c7a\u8b70\uff1a     (\u4e00) \u767d\u6cb3\u5340\u516c\u6240\uff1a         1\u3001072-28\u9801\uff0c\u696d\u52d9\u8cbb-\u4e00\u822c\u4e8b\u52d9\u8cbb-\u8fa6\u7406\u57ce\u9109\u4ea4\u6d41\u6d3b\u52d5\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e6356\u842c\u5143\uff0c\u8fa6\u7406\u57ce\u9109\u4ea4\u6d41\u6d3b\u52d5\u7d93\u8cbb\uff0c\u4fee\u6b63\u70ba\u5176\u4e2d10\u842c\u5143\u4f75\u70ba\u5340\u516c\u6240\u91cc\u9130\u9577\u6587\u5eb7\u6d3b\u52d5\u7d93\u8cbb\uff0c\u5176\u9918\u7d93\u8cbb\u88dc\u52a9\u5404\u91cc\u81ea\u884c\u8fa6\u7406\u4ea4\u6d41\u6d3b\u52d5\u3002         2\u3001072-37\u9801\uff0c\u696d\u52d9\u8cbb-\u4e00\u822c\u4e8b\u52d9\u8cbb-\u8fa6\u7406\u5916\u7c4d\u914d\u5076\u806f\u8abc\u6d3b\u52d5\u3001\u5927\u9678\u914d\u5076\u806f\u8abc\u6d3b\u52d5\u3001\u5bd2\u51ac\u9001\u6696\u6d3b\u52d5\u53ca\u8868\u63da\u6d3b\u52d5\u7b49\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e6355\u842c\u5143\uff0c\u8fa6\u7406\u5916\u7c4d\u914d\u5076\u806f\u8abc\u6d3b\u52d5\u3001\u5927\u9678\u914d\u5076\u806f\u8abc\u6d3b\u52d5\u3001\u5bd2\u51ac\u9001\u6696\u6d3b\u52d5\u53ca\u8868\u63da\u6d3b\u52d5\u7b49\uff0c\u4fee\u6b63\u70ba\u7531\u5404\u91cc\u81ea\u884c\u8fa6\u7406\u91cc\u6c11\u8207\u65b0\u4f4f\u6c11\u806f\u8abc\u6d3b\u52d5\u3002     (\u4e8c) \u52de\u5de5\u5c40\uff1a         1\u3001751-17\u9801\uff0c\u696d\u52d9\u8cbb-\u6309\u65e5\u6309\u4ef6\u8a08\u8cc7\u916c\u91d1-\u52de\u8cc7\u722d\u8b70\u7368\u4efb\u8abf\u89e3\u4eba\u3001\u8abf\u89e3\u59d4\u54e1\u51fa\u5e2d\u8cbb\u3001\u4ef2\u88c1\u4eba\u53ca\u8abf\u67e5\u722d\u8b70\u6848\u4ef6\u5be9\u67e5\u51fa\u5e2d\u7b49\u76f8\u95dc\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e63110\u842c\u5143\uff0c\u5e02\u5e9c\u65bc\u52de\u8cc7\u722d\u8b70\u6848\u4ef6\u8655\u7406\u4e2d\uff0c\u61c9\u4e3b\u52d5\u5354\u52a9\u52de\u5de5\u586b\u59a5\u5b8c\u6574\u722d\u8b70\u4e3b\u5f35\u3002         2\u3001751-30\u9801\uff0c\u8a2d\u5099\u53ca\u6295\u8cc7-\u96dc\u9805\u8a2d\u5099\u8cbb-\u5357\u9580\u52de\u5de5\u80b2\u6a02\u4e2d\u5fc3\u6c23\u51b7\u5f0f\u51b7\u6c23\u6a5f\u3001\u5c0f\u578b\u9001\u98a8\u6a5f\u3001\u51b0\u6c34\u7ba1\u8def\u4fdd\u6fd5\u3001\u5ec1\u6240\u4fbf\u5668\u7b49\u885b\u751f\u76f8\u95dc\u8a2d\u5099\u7dad\u4fee\u6c70\u63db\u9810\u7b97\u6578\u65b0\u81fa\u5e63120\u842c\u5143\uff0c\u5e02\u5e9c\u61c9\u65bc103\u5e743\u6708\u524d\u767c\u5305\u5b8c\u7562\u3002     (\u4e09) \u81fa\u5357\u5e02\u8b70\u6703\uff1a            001-24\u9801\uff0c\u696d\u52d9\u8cbb-\u4e00\u822c\u4e8b\u52d9\u8cbb-\u8fa6\u7406\u672c\u6703\u65b0\u805e\u696d\u52d9(\u542b\u65b0\u805e\u7a3f\u4ef6\u50b3\u771f\u8207\u5916\u7e23\u5e02\u96fb\u5831)\u7b49\u5404\u9805\u5de5\u4f5c\u6240\u9700\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e63400\u842c\u5143\uff0c\u5404\u59d4\u54e1\u6703\u5916\u52d8\u4e4b\u6703\u8b70\u7d50\u8ad6\u9001\u516c\u5171\u4e8b\u52d9\u7d44\u7d71\u4e00\u767c\u4f48\u65b0\u805e\u7a3f\u3002     (\u56db) \u6587\u5316\u5c40\uff1a         1\u3001551-32\u9801\uff0c\u696d\u52d9\u8cbb-\u59d4\u8fa6\u8cbb-\u85dd\u6587\u666e\u67e5\u7b49\u8cbb\u7528\u9810\u7b97\u6578\u65b0\u81fa\u5e63100\u842c\u5143\uff0c\u522a\u6e1b99\u842c9,000\u5143\uff0c\u4fdd\u7559\u9810\u7b97\u79d1\u76ee1,000\u5143\uff0c\u8acb\u6587\u5316\u5c40\u5c07\u85dd\u6587\u666e\u67e5\u660e\u7d30\u53ca\u6210\u679c\u9001\u6703\u3002         2\u3001551-32\u9801\uff0c\u696d\u52d9\u8cbb-\u4e00\u822c\u4e8b\u52d9\u8cbb-\u8fa6\u7406\u81fa\u5357\u771f\u6709\u6625\u7cfb\u5217-\u5e9c\u524d\u5ee3\u5834\u6f14\u51fa\u6d3b\u52d5\u7b49\u8cbb\u7528\u9810\u7b97\u6578\u65b0\u81fa\u5e63282\u842c\u5143\uff0c\u4e0d\u5f97\u8fa6\u7406\u300c\u6cd5\u570bGroupe F\u706b\u5287\u5718\u300d\u6d3b\u52d5\u3002         3\u3001551-70\u9801\uff0c\u8a2d\u5099\u53ca\u6295\u8cc7-\u623f\u5c4b\u5efa\u7bc9\u53ca\u8a2d\u5099\u8cbb-\u81fa\u5357\u5e02\u7f8e\u8853\u9928\u8208\u8a2d\u5de5\u7a0b\u59d4\u8a17\u570b\u969b\u5fb5\u5716\u3001\u5c08\u6848\u7ba1\u7406\u53ca\u76e3\u9020\u7b49\u8a08\u756b\u9810\u7b97\u6578\u65b0\u81fa\u5e63100\u842c\u5143\uff0c(1)\u8aaa\u660e\u6b04\u6587\u5b57\u4fee\u6b63\u70ba102\u5e74\u5ea6\u5167\u5bb9\uff1a\u81fa\u5357\u5e02\u7f8e\u8853\u9928\u8208\u8a2d\u5de5\u7a0b\u5c08\u6848\u7ba1\u7406\u8a08\u756b(\u7e3d\u7d93\u8cbb5,584\u842c\u5143\uff0c\u63a1\u4e00\u6b21\u767c\u5305\uff0c\u5206\u5e74\u7de8\u5217\u9810\u7b97\u8fa6\u7406\uff0c102\u5e74\u5ea6\u7de8\u52171,500\u842c\u5143\uff0c103\u5e74\u81f3105\u5e74\u5ea6\u6bcf\u5e74\u5ea6\u7de8\u52171,050\u842c\u5143\uff0c106\u5e74\u5ea6\u7de8\u5217934\u842c\u5143)\u3002(2)\u4f9d102\u5e74\u5ea6\u8aaa\u660e\u6b04\u6587\u5b57\u6240\u5217103\u5e74\u7de8\u5217\u6578\u61c9\u70ba1,050\u842c\u5143\uff0c\u4eca\u5e74\u5ea6\u901a\u904e\u91d1\u984d100\u842c\u5143\uff0c\u9810\u7b97\u4e0d\u8db3\u984d\u90e8\u5206\u8acb\u6587\u5316\u5c40\u8fa6\u7406\u8ffd\u52a0\u9810\u7b97\u3002         4\u3001\u81fa\u5357\u5e02\u6587\u5316\u5efa\u8a2d\u767c\u5c55\u57fa\u91d1\u7b2c13\u9801\uff0c\u4e00\u822c\u670d\u52d9\u8cbb-\u7bc0\u76ee\u6f14\u51fa\u8cbb-\u8fa6\u7406\u6587\u5316\u6d3b\u52d5\u8868\u6f14\u7b49\u8cbb\u7528\u9810\u7b97\u6578\u65b0\u81fa\u5e63800\u842c\u5143\uff0c\u4e0d\u5f97\u8fa6\u7406\u300c\u6cd5\u570bGroupe F\u706b\u5287\u5718\u300d\u6d3b\u52d5\u3002   (\u4e94) \u6559\u80b2\u5c40\uff1a         1\u3001\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100010-16\u9801\uff0c\u52a0\u73ed\u8cbb-\u8fa6\u7406\u696d\u52d9\u6240\u9700\u52a0\u73ed\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e6325\u842c\u5143\uff0c\u6559\u80b2\u5c40103\u5e74\u5ea6\u6574\u9ad4\u52a0\u73ed\u8cbb\u4e0d\u5f97\u8d85\u904e102\u5e74\u5ea6\u52a0\u73ed\u8cbb\u7e3d\u984d\u3002            2\u3001\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100013-12\u9801(\u81fa\u5357\u5e02\u9ad4\u80b2\u8655)\uff0c\u6350\u52a9\u79c1\u6821\u53ca\u5718\u9ad4-\u88dc\u52a9\u5404\u9805\u570b\u969b\u53ca\u5168\u570b\u6027\u9326\u6a19\u8cfd\u6d3b\u52d5\u7d93\u8cbb\uff08\u542b\u9435\u4eba\u4e09\u9805\u3001\u81ea\u7531\u8eca\u74b0\u53f0\u8cfd\u53ca\u83ef\u5b97\u76c3\u6392\u7403\u8cfd\u7b49\uff09\u9810\u7b97\u6578\u65b0\u81fa\u5e63725\u842c\u5143\uff0c\u300c\u9435\u4eba\u4e09\u9805\u300d\u4e0d\u5f97\u5728\u89c0\u5915\u5e73\u81fa\u7b49\u5371\u96aa\u6d77\u57df\u8209\u8fa6\uff0c\u5426\u5247\u7d93\u8cbb\u4e0d\u5f97\u52d5\u652f\u3002            3\u3001\u5176\u4ed6\uff1a\u793e\u5718\u6cd5\u4eba\u81fa\u5357\u5e02\u6559\u5e2b\u6703\u53ca\u81fa\u5357\u5e02\u6559\u80b2\u7522\u696d\u5de5\u6703\u7684\u6559\u5e2b\u6e1b\u6388\u8ab2\u9418\u9ede\u8cbb\u4e0d\u4e88\u88dc\u52a9\u3002        (\u516d) \u8fb2\u696d\u5c40\uff1a         1\u3001601-67\u9801\uff0c\u734e\u88dc\u52a9\u8cbb-\u734e\u52f5\u53ca\u6170\u554f-\u8fb2\u5730\u7ba1\u7406\u6aa2\u8209\u734e\u91d1\u9810\u7b97\u6578\u65b0\u81fa\u5e632\u842c4,000\u5143\uff0c\u522a\u6e1b2\u842c3,000\u5143\uff0c\u4fdd\u7559\u9810\u7b97\u79d1\u76ee\u3002         2\u3001602-35\u9801(\u52d5\u7269\u9632\u75ab\u4fdd\u8b77\u8655)\uff0c\u8a2d\u5099\u53ca\u6295\u8cc7-\u623f\u5c4b\u5efa\u7bc9\u53ca\u8a2d\u5099\u8cbb-\u5584\u5316\u52d5\u7269\u4e4b\u5bb6\u642c\u9077\u5efa\u8a2d\u53ca\u8cfc\u7f6e\u8a2d\u5099\u8cbb\u7528\u9810\u7b97\u6578\u65b0\u81fa\u5e63500\u842c\u5143\uff0c\u522a\u9664\u300c\u5584\u5316\u300d2\u5b57\u3002         3\u3001\u81fa\u5357\u5e02\u8fb2\u696d\u767c\u5c55\u57fa\u91d1\u7b2c7\u9801\uff0c\u5176\u4ed6-\u8fb2\u5730\u9055\u898f\u4f7f\u7528\u6aa2\u8209\u734e\u91d1\u9810\u7b97\u6578\u65b0\u81fa\u5e636\u842c\u5143\uff0c\u522a\u6e1b5\u842c7,000\u5143\uff0c\u4fdd\u7559\u9810\u7b97\u79d1\u76ee\u3002         4\u3001\u5176\u4ed6\uff1a\u8fb2\u696d\u5c40\u52d5\u7269\u9632\u75ab\u4fdd\u8b77\u8655\u6d41\u6d6a\u8c93\u72d7\u64b2\u6bba\u4e0d\u5f97\u8d85\u904e2000\u96bb\uff0c\u5269\u9918\u6b3e\u7528\u5728\u7d55\u80b2\u76f8\u95dc\u8cbb\u7528\u3002    (\u4e03) \u7d93\u6fdf\u767c\u5c55\u5c40\uff1a         1\u3001650-29\u9801\uff0c\u696d\u52d9\u8cbb-\u59d4\u8fa6\u8cbb-\u59d4\u8a17\u8fa6\u7406\u5e74\u8ca8\u5927\u8857\u7b49\u76f8\u95dc\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e63230\u842c\u5143\uff0c\u589e\u52a0\u300c\u65b0\u5316\u300d2\u5b57\uff0c\u9664\u8fa6\u7406\u65b0\u5316\u5e74\u8ca8\u5927\u8857\u5916\uff0c\u4e0d\u5f97\u52d5\u652f\u3002         2\u3001650-29\u9801\uff0c\u696d\u52d9\u8cbb-\u4e00\u822c\u4e8b\u52d9\u8cbb-\u9810\u7b97\u6578\u65b0\u81fa\u5e632,390\u842c8,000\u5143\uff0c\u5c07\u4e00\u822c\u4e8b\u52d9\u8cbb\u9810\u7b97\u7de8\u5217\u7d30\u7bc0\u9001\u8b70\u6703\u3002         3\u3001650-36\u9801\uff0c\u696d\u52d9\u8cbb-\u571f\u5730\u79df\u91d1-\u652f\u4ed8\u6210\u529f\u5927\u5b78\u3001\u4e2d\u7814\u9662\u9032\u99d0\u5357\u79d1\u571f\u5730\u79df\u91d1\u9810\u7b97\u6578\u65b0\u81fa\u5e631,040\u842c\u5143\uff0c\u522a\u6e1b132\u842c8,000\u5143\uff0c\u4e0d\u8db3\u90e8\u5206\u8acb\u696d\u52d9\u55ae\u4f4d\u518d\u8ffd\u52a0\u9810\u7b97\u8fa6\u7406\u3002    (\u516b) \u885b\u751f\u5c40\uff1a            760-58\u9801\uff0c\u734e\u88dc\u52a9\u8cbb-\u5176\u4ed6\u88dc\u52a9\u53ca\u6350\u52a9-\u88dc\u52a965\u6b72\u4ee5\u4e0a\u5e02\u6c11\u5168\u53e3\u5047\u7259\u88dd\u7f6e\u9810\u7b97\u6578\u65b0\u81fa\u5e632\u51049,822\u842c7,000\u5143\uff0c\u8acb\u885b\u751f\u5c40\u5236\u5b9a\u6392\u5bcc\u689d\u6b3e\u9001\u8b70\u6703\uff0c104\u5e74\u5ea6\u57f7\u884c\u3002    (\u4e5d) \u74b0\u5883\u4fdd\u8b77\u5c40\uff1a            820-49\u9801\uff0c\u696d\u52d9\u8cbb-\u59d4\u8fa6\u8cbb-\u9810\u7b97\u6578\u65b0\u81fa\u5e632,435\u842c  3,000\u5143\uff0c(1)101\u3001102\u5e74\u59d4\u8a17\u8fa6\u7406\u571f\u58e4\u53ca\u5730\u4e0b\u6c34\u6c61\u67d3\u67e5\u6838\u8a08\u756b\u5831\u544a\u9001\u8b70\u6703\u3002(2)\u91dd\u5c0d\u59d4\u8fa6\u8cbb\u90e8\u5206\uff0c\u8acb\u74b0\u4fdd\u5c40\u65bc12\u67089\u65e5\u5b9a\u671f\u5927\u6703\u7d50\u675f\u5f8c\u4f5c\u5c08\u6848\u5831\u544a\uff0c\u7531\u5efa\u8a2d\u59d4\u54e1\u6703\u53ec\u958b\u4e26\u901a\u77e5\u6240\u6709\u8b70\u54e1\u53c3\u52a0\u3002(3)\u91dd\u5c0d\u4e2d\u77f3\u5316\u6c61\u67d3\u554f\u984c\uff0c\u8acb\u74b0\u4fdd\u5c40\u6703\u540c\u8b70\u54e1\u5230\u5834\u6703\u52d8\u3002          3\u3001820-54\u9801\uff0c\u696d\u52d9\u8cbb-\u59d4\u8fa6\u8cbb-\u4e2d\u897f\u5340\u3001\u5b89\u5e73\u5340\u53ca\u516c\u6709\u5e02\u5834\u5783\u573e\u6e05\u904b\u6c11\u71df\u5316\u8a08\u756b\u9810\u7b97\u6578\u65b0\u81fa\u5e634,480\u842c\u5143\uff0c\u59d4\u5916\u904e\u7a0b\u9001\u6aa2\u8abf\u8abf\u67e5\u3002         4\u3001820-59\u9801\uff0c\u696d\u52d9\u8cbb-\u59d4\u8fa6\u8cbb-\u81fa\u5357\u5e02\u5783\u573e\u8cc7\u6e90\u8655\u7406\u5ee0\u4e4b\u98db\u7070\u56fa\u5316\u5ee0\u59d4\u5916\u64cd\u4f5c\u71df\u904b\u7ba1\u7406\u8a08\u756b\u9810\u7b97\u6578\u65b0\u81fa\u5e632,600\u842c\u5143\uff0c\u59d4\u5916\u64cd\u4f5c\u71df\u904b\u7ba1\u7406\u8a08\u756b\u76f8\u95dc\u8cc7\u6599\u9001\u8b70\u6703\u3002    (\u5341) \u81fa\u5357\u5e02\u653f\u5e9c\u8b66\u5bdf\u5c40\uff1a            871-30\u9801\uff0c\u8a2d\u5099\u53ca\u6295\u8cc7-\u96dc\u9805\u8a2d\u5099\u8cbb-\u6cbb\u5b89\u76e3\u8996\u9304\u5f71\u7cfb\u7d71\u6a5f\u7d44\u88dd\u8a2d\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e631\u51045,700\u842c\u5143\uff0c\u76e3\u8996\u5668\u63a1\u8cfc\u5269\u9918\u6b3e\uff0c\u5fc5\u9808\u7528\u65bc\u76e3\u8996\u5668\u7d44\u88dd\uff0c\u5e02\u5e9c\u4e0d\u5f97\u6536\u56de\u3002    (\u5341\u4e00) \u6c34\u5229\u5c40\uff1a             610-32\u9801\uff0c\u8a2d\u5099\u53ca\u6295\u8cc7-\u516c\u5171\u5efa\u8a2d\u53ca\u8a2d\u65bd\u8cbb-\u8fa6\u7406\u904b\u6cb3\u4e4b\u5fc3\u6e2f\u7063\u95e2\u5efa\u66a8\u6d6e\u52d5\u78bc\u982d\u8a2d\u7f6e\u5de5\u7a0b\u7d93\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e635,990\u842c\u5143\uff0c\u8fa6\u7406\u904b\u6cb3\u4e4b\u5fc3\u6e2f\u7063\u95e2\u5efa\u66a8\u6d6e\u52d5\u78bc\u982d\u8a2d\u7f6e\u5de5\u7a0b\u9810\u7b975,990\u842c\u5143\uff0c\u9808\u4fdf\u89c0\u5149\u65c5\u904a\u5c40BOT\u6848\u62db\u6a19\u4e0d\u6210\uff0c\u7531\u5e02\u5e9c\u81ea\u884c\u8fa6\u7406\u65bd\u5de5\u71df\u904b\u65b9\u5f97\u52d5\u652f\uff0c\u4e14\u4e0d\u5f97\u4ee5\u7a92\u7919\u96e3\u884c\u7b49\u7406\u7531\u884c\u6587\u672c\u6703\u8981\u6c42\u8fa6\u7406\u3002     (\u5341\u4e8c) \u5730\u653f\u5c40\uff1a            \u5176\u4ed6\uff1a\u5730\u653f\u5c40\u4e3b\u7ba1\u81fa\u5357\u5e02\u5be6\u65bd\u5e73\u5747\u5730\u6b0a\u57fa\u91d1\uff1a\u6c34\u4ea4\u793e\u5e02\u5730\u91cd\u5283\u696d\u52d9-\u5de5\u7a0b\u8cbb\u9810\u7b97\u57f7\u884c\u7387\u6700\u5c11\u9700\u905490\uff05\u3002     (\u5341\u4e09) \u5de5\u52d9\u5c40\uff1a            \u5176\u4ed6\uff1a104\u5e74\u767e\u82b1\u796d\u4e0d\u5f97\u5728\u81fa\u5357\u516c\u5712\u8fa6\u7406\uff0c\u53e6\u8993\u9069\u7576\u5730\u9ede\u8fa6\u7406\u4e26\u6ce8\u610f\u5916\u4f86\u83cc\u7a2e\u554f\u984c\u3002     (\u5341\u56db) \u5176\u4ed6\uff1a(\u5fa9\u8b70\u901a\u904e\u5f8c\u65b0\u589e)         1\u3001820-31\u9801(\u74b0\u4fdd\u5c40)\uff0c\u696d\u52d9\u8cbb-\u5176\u4ed6\u696d\u52d9\u79df\u91d1-\u6771\u5340\u7b2c\u4e8c\u8fa6\u516c\u5ba4\u79df\u91d1\u9810\u7b97\u6578\u65b0\u81fa\u5e63230\u842c4,000\u5143\uff0c\u6c11\u570b105\u5e74\u4ee5\u524d\u61c9\u5c0b\u627e\u9069\u7576\u5730\u9ede\u81ea\u884c\u8208\u5efa\u3002         2\u3001\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u57fa\u91d1(P00010-20\u9801\u81f3P00010-21\u9801) 103\u5e74\u5ea6\u6559\u80b2\u5c40-\u570b\u6c11\u5c0f\u5b78\u6559\u80b2\uff0d\u570b\u6c11\u5c0f\u5b78\u6559\u80b2\u884c\u653f\u53ca\u7763\u5c0e\uff0c\u670d\u52d9\u8cbb\u7528\u9805\u4e0b\u6240\u5c6c\u8cc7\u8a0a\u4e2d\u5fc3\u71df\u904b\u6709\u95dc\u9810\u7b97(\u7d30\u76ee\u5982\u4e0b)\u5171\u8a08\u65b0\u81fa\u5e63199\u842c6,000\u5143\uff0c\u6c11\u570b107\u5e74\u4ee5\u5f8c\u501f\u8abf\u8001\u5e2b\u61c9\u5168\u90e8\u6b78\u5efa\u3002","\u8fa6\u7406\u60c5\u5f62":"\u672c\u6848\u7d93\u5e9c103.01.14\u5e9c\u6559\u6703\u5b57\u7b2c1021176516\u865f\u51fd\u5fa9\uff1a\u6709\u95dc\u8ca1\u7d931\u865f\u6848\u7b2c\u4e09\u9ede\u9644\u5e36\u6c7a\u8b70\u7b2c\u4e94\u9805\u7b2c\u4e00\u6b3e\uff0c\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100010-16\u9801\uff0c\u52a0\u73ed\u8cbb-\u8fa6\u7406\u696d\u52d9\u6240\u9700\u52a0\u73ed\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e6325\u842c\u5143\uff0c\u6559\u80b2\u5c40103\u5e74\u5ea6\u6574\u9ad4\u52a0\u73ed\u8cbb\u4e0d\u5f97\u8d85\u904e102\u5e74\u5ea6\u52a0\u73ed\u8cbb\u7e3d\u984d\uff0c\u672c\u5e9c\u4f9d\u6c7a\u8b70\u5167\u5bb9\u8fa6\u7406\u3002-----------------------------\u672c\u6848\u7d93\u5e9c103.01.20\u5e9c\u6559\u8ab2(\u4e8c)\u5b57\u7b2c1030020537\u865f\u51fd\u5fa9\uff1a\u4e00\u3001\u6709\u95dc\u9644\u5e36\u6c7a\u8b70(\u4e94)\u6559\u80b2\u5c40\uff1a3\u3001\u5176\u4ed6\uff1a\u793e\u5718\u6cd5\u4eba\u81fa\u5357\u5e02\u6559\u5e2b\u6703\u53ca\u81fa\u5357\u5e02\u6559\u80b2\u7522\u696d\u5de5\u6703\u7684\u6559\u5e2b\u6e1b\u6388\u8ab2\u9418\u9ede\u8cbb\u4e0d\u4e88\u88dc\u52a9\u3002\u8aaa\u660e\u5982\u4e0b\uff1a(\u4e00)\u672c\u5e02\u6240\u5c6c\u5404\u7d1a\u5b78\u6821\u53c3\u8207\u793e\u5718\u6cd5\u4eba\u81fa\u5357\u5e02\u6559\u5e2b\u6703\u53ca\u81fa\u5357\u5e02\u6559\u80b2\u7522\u696d\u5de5\u6703\u7684\u6559\u5e2b\u903e8000\u4eba\uff0c\u7686\u4f9d\u300c\u672c\u5e02\u6559\u5e2b\u66a8\u517c\u4efb\u884c\u653f\u4eba\u54e1\u6bcf\u9031\u6388\u8ab2\u7bc0\u6578\u3001\u5b78\u751f\u6bcf\u9031\u5b78\u7fd2\u7bc0\u6578\u88dc\u5145\u898f\u5b9a\u300d\u6392\u8ab2\uff0c\u60df\u672c\u5e9c\u6559\u80b2\u5c40\u53e6\u6709\u6838\u5b9a\u6e1b\u6388\u8ab2\u7bc0\u6578\u8005\u4e0d\u5728\u6b64\u9650\u3002(\u4e8c)\u6709\u95dc\u6559\u80b2\u7522\u696d\u5de5\u6703\u6703\u52d9\u516c\u5047\u76f8\u95dc\u554f\u984c\uff0c\u70ba\u5168\u570b\u6027\u4e00\u81f4\u4e4b\u554f\u984c\uff0c\u672c\u5e9c\u5df2\u65bc102\u5e7411\u670829\u65e5\u53d7\u76e3\u5bdf\u9662\u7d04\u8a62\uff0c\u5c07\u4fdf\u76e3\u5bdf\u9662\u8abf\u67e5\u7d50\u679c\u4f86\u51fd\u5f8c\uff0c\u8207\u5168\u570b\u5404\u7e23\u5e02\u4f5c\u4e00\u81f4\u6027\u901a\u76e4\u6aa2\u8a0e\uff0c\u4e26\u5df2\u5c07\u6b64\u8b70\u984c\u7d0d\u5165\u8207\u6559\u80b2\u7522\u696d\u5de5\u6703\u5718\u9ad4\u5354\u7d04\u5354\u5546\u8b70\u7a0b\u3002\u70ba\u8b93\u672c\u5e9c\u6240\u5c6c\u5404\u7d1a\u5b78\u6821\u7684\u8ab2\u52d9\u5b89\u6392\u5b8c\u6574\u6027\u53ca\u5b78\u751f\u8ab2\u7a0b\u53d7\u6559\u6b0a\u7684\u5ef6\u7e8c\u6027\uff0c\u64ec\u7dad\u6301\u73fe\u6709\u6e1b\u6388\u8ab2\u65b9\u5f0f\u4e0d\u505a\u7570\u52d5\uff0c103\u5b78\u5e74\u5ea6(103\u5e748\u67081\u65e5)\u518d\u505a\u901a\u76e4\u6aa2\u8a0e\u3002----------------------------- \u672c\u6848\u7d93\u5e9c103.03.13\u5e9c\u6559\u9ad4\u8655\u7e3d\u5b57\u7b2c1030190221\u865f\u51fd\u5fa9\uff1a\u4e00\u3001\u6709\u95dc\u8ca1\u7d931\u865f\u6848\u7b2c\u4e09\u9ede\u9644\u5e36\u6c7a\u8b70\u7b2c\u4e94\u9805\u7b2c\u4e8c\u6b3e\uff0c\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100013-12\u9801(\u81fa\u5357\u5e02\u9ad4\u80b2\u8655)\uff0c\u6350\u52a9\u79c1\u6821\u53ca\u5718\u9ad4-\u88dc\u52a9\u5404\u9805\u570b\u969b\u53ca\u5168\u570b\u6027\u9326\u6a19\u8cfd\u6d3b\u52d5\u7d93\u8cbb\uff08\u542b\u9435\u4eba\u4e09\u9805\u3001\u81ea\u7531\u8eca\u74b0\u53f0\u8cfd\u53ca\u83ef\u5b97\u76c3\u6392\u7403\u8cfd\u7b49\uff09\u9810\u7b97\u6578\u65b0\u81fa\u5e63725\u842c\u5143\uff0c\u300c\u9435\u4eba\u4e09\u9805\u300d\u4e0d\u5f97\u5728\u89c0\u5915\u5e73\u81fa\u7b49\u5371\u96aa\u6d77\u57df\u8209\u8fa6\uff0c\u5426\u5247\u7d93\u8cbb\u4e0d\u5f97\u52d5\u652f\u3002\u4e8c\u3001\u672c\u5e9c\u696d\u4f9d\u6c7a\u8b70\u5167\u5bb9\u8fa6\u7406\uff0c\u672c\u6848\u6bd4\u8cfd\u5df2\u6539\u65bc\u70cf\u5c71\u982d\u6c34\u5eab\u8209\u8fa6\uff0c\u8332\u6aa2\u9644\u7af6\u8cfd\u898f\u7a0b\u4e59\u4efd\u4f9b\u53c3\u3002-------------------------------\u672c\u6848\u7d93\u5e9c103.03.17\u5e9c\u6559\u8ab2(\u4e8c)\u5b57\u7b2c1030229445\u865f\u51fd\u5fa9\uff1a\u4e00\u3001\u6709\u95dc\u9644\u5e36\u6c7a\u8b70(\u5341\u56db)\uff1a(\u5fa9\u8b70\u901a\u904e\u5f8c\u65b0\u589e)\u7b2c2\u9ede\uff1a\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u57fa\u91d1(P00010-20\u9801\u81f3P00010-21\u9801)103\u5e74\u5ea6\u6559\u80b2\u5c40-\u570b\u6c11\u5c0f\u5b78\u6559\u80b2-\u570b\u6c11\u5c0f\u5b78\u6559\u80b2\u884c\u653f\u53ca\u7763\u5c0e\uff0c\u670d\u52d9\u8cbb\u7528\u9805\u4e0b\u6240\u5c6c\u8cc7\u8a0a\u4e2d\u5fc3\u71df\u904b\u6709\u95dc\u9810\u7b97(\u7d30\u76ee\u5982\u4e0b)\u5171\u8a08\u65b0\u81fa\u5e63199\u842c6,000\u5143\uff0c\u6c11\u570b107\u5e74\u4ee5\u5f8c\u501f\u8abf\u8001\u5e2b\u61c9\u5168\u90e8\u6b78\u5efa\u3002\u8aaa\u660e\u5982\u4e0b\uff1a    (\u4e00)\u70ba\u4f7f\u672c\u5e02\u8abf\u7528\u6559\u5e2b\u7a0b\u5e8f\u5236\u5ea6\u5316\uff0c\u672c\u5e02\u5df2\u65bc102\u5e748\u670819\u65e5\u51fd\u9812\u300c\u81fa\u5357\u5e02\u653f\u5e9c\u6559\u80b2\u5c40\u8abf\u7528\u6559\u5e2b\u8655\u7406\u539f\u5247\u300d\uff0c\u64da\u4ee5\u898f\u7bc4\u672c\u5e02\u6559\u5e2b\u8abf\u7528\u4e8b\u5b9c\u3002    (\u4e8c)\u672c\u5e02\u6559\u80b2\u696d\u52d9\u4ee5\u53ca\u6559\u80b2\u653f\u7b56\u898f\u5283\u4ecd\u6709\u5fc5\u8981\u501f\u91cd\u8abf\u7528\u6559\u5e2b\u6559\u5b78\u73fe\u5834\u4e4b\u7d93\u9a57\uff0c\u4e14\u8abf\u7528\u6559\u5e2b\u4fc2\u4ee5\u5c0a\u91cd\u6559\u5e2b\u610f\u9858\u524d\u63d0\u4e0b\u5b8c\u6210\uff0c\u63d0\u4f9b\u5176\u6559\u5b78\u7d93\u9a57\u5354\u52a9\u672c\u5e02\u6559\u80b2\u696d\u52d9\uff0c\u4fc3\u4f7f\u6559\u80b2\u653f\u7b56\u843d\u5be6\u65bc\u5b78\u6821\u53ef\u7121\u7e2b\u63a5\u8ecc\u3002    (\u4e09)\u70ba\u7dad\u8b77\u5b78\u751f\u53d7\u6559\u6b0a\uff0c\u672c\u5c40\u63a7\u7ba1\u8abf\u7528\u6559\u5e2b\u54e1\u984d\uff0c\u4f9d\u300c\u81fa\u5357\u5e02\u653f\u5e9c\u6559\u80b2\u5c40\u8abf\u7528\u6559\u5e2b\u8655\u7406\u539f\u5247\u300d\u7b2c\u4e94\u9ede\u898f\u5b9a\uff0c\u6bcf\u6821\u8abf\u7528\u6559\u5e2b\u4eba\u6578\u81f3\u591a3\u4eba\uff0c \u7de8\u523620\u4eba\u4ee5\u4e0b\u5b78\u6821\u4ee51\u4eba\u70ba\u9650\u3002---------------------------------\u4e3b\u8a08\u8655\uff1a1.\u6709\u95dc103\u5e74\u5ea6\u5730\u65b9\u7e3d\u9810\u7b97\u5be9\u8b70\u6536\u5165\u3001\u652f\u51fa\u522a\u6e1b\u5dee\u984d2\u51048,606\u842c3,000\u5143\u90e8\u5206\uff0c\u672c\u5e9c\u8abf\u6574\u522a\u6e1b\u6b72\u51fa\u9810\u7b97\u4e4b\u7d71\u7c4c\u652f\u64a5\u79d1\u76ee-\u516c\u52d9\u4eba\u54e1\u9000\u4f11\u7d66\u4ed82\u51048,606\u842c3,000\u5143\uff0c\u4ffe\u4f7f\u6536\u652f\u5e73\u8861\uff0c\u8abf\u6574\u5f8c\u6b72\u5165\u9810\u7b97\u6578\u70ba732\u51049,928\u842c5,000\u5143\u3001\u6b72\u51fa\u9810\u7b97\u6578\u70ba800\u51049,928\u842c5,000\u5143\uff0c\u6b72\u5165\u3001\u6b72\u51fa\u5dee\u77ed68\u5104\u5143\uff0c\u5dee\u77ed\u90e8\u5206\uff0c\u4ee5\u8209\u501f\u540c\u984d\u501f\u6b3e\u652f\u61c9\u30022.\u672c\u8655\u5df2\u65bc102\u5e7412\u670819\u65e5\u4ee5\u5e9c\u4e3b\u9810\u5b57\u7b2c1021111950\u865f\u51fd\u5c07\u5e02\u8b70\u6703\u5be9\u8b70103\u5e74\u5ea6\u5730\u65b9\u7e3d\u9810\u7b97\u6848\u53ca\u5730\u65b9\u7e3d\u9810\u7b97\u6848\u9644\u5c6c\u55ae\u4f4d\u9810\u7b97\u53ca\u7d9c\u8a08\u8868\u7d50\u679c\u901a\u77e5\u5404\u6a5f\u95dc\u5b78\u6821\uff0c\u4e26\u8acb\u5404\u6a5f\u95dc\u5b78\u6821\u4f9d103\u5e74\u5ea6\u7e3d\u9810\u7b97\u5206\u914d\u53ca\u57f7\u884c\u539f\u5247\u8fa6\u7406\u9810\u7b97\u5206\u914d\u53ca\u57f7\u884c\u3002\u8ca1\u653f\u8655\uff1a\u672c\u6b21\u5b9a\u671f\u5927\u6703\u8b70\u6c7a\u672c\u5e9c\u55ae\u4f4d\u9810\u7b97\u522a\u6e1b\u6b72\u5165\u9810\u7b973\u5104\u5143\uff08\u9810\u7b97\u55ae\u4f4d\uff1a\u8b66\u5bdf\u5c40\uff09\uff0c\u522a\u6e1b\u6b72\u51fa\u9810\u7b971,393\u842c7,000\u5143\u3002\u6536\u652f\u5dee\u984d\u90e8\u5206\uff0c\u4e3b\u8a08\u8655\u4f9d\u8b70\u6c7a\u4e8b\u9805(\u4e09)\uff0c\u8abf\u6574\u522a\u6e1b\u6b72\u51fa\u9810\u7b97\u4e4b\u7d71\u7c4c\u652f\u64a5\u79d1\u76ee\uff0d\u516c\u52d9\u4eba\u54e1\u9000\u4f11\u7d66\u4ed82\u51048,606\u842c3,000\u5143\uff0c\u4ffe\u4f7f\u6536\u652f\u5e73\u8861\u3002\u8fb2\u696d\u5c40\uff1a1.\u52d5\u7269\u4e4b\u5bb6\u642c\u9077\u5efa\u8a2d\u8cbb\u7528\uff0c\u6b63\u7814\u64ec\u898f\u5283\u66ff\u4ee3\u65b9\u6848\u30022.\u76ee\u524d\u5b89\u6a02\u6b7b\u6578\u91cf\u9075\u7167\u8b70\u6703\u6c7a\u8b70\u9650\u5b9a2000\u982d\u4ee5\u5167\uff0c\u9810\u4f30\u672c\u5e74\u5728\u300c\u653f\u5e9c\u63a8\u884cTNA\u53ca\u88dc\u52a9\u6c11\u9593\u5718\u9ad4\u57f7\u884cTNR\u300d\u63aa\u65bd\u4e0b\uff0c\u5c07\u6709\u6a5f\u6703\u9054\u6210\u76ee\u6a19\u30023.\u8fb2\u5730\u9055\u898f\u4f7f\u7528\u6aa2\u8209\u734e\u91d1\u9810\u7b97\u4f9d\u6c7a\u8b70\u8fa6\u7406\u3002\u767d\u6cb3\u5340\u516c\u6240\uff1a10\u842c\u64ec\u4f75\u5165\u91cc\u9130\u9577\u6587\u5eb7\u6d3b\u52d5\u7d93\u8cbb\u904b\u7528\uff08\u6d3b\u52d5\u9810\u8a08103\u5e743\/4~3\/5\u30013\/10~3\/11\u4e8c\u68af\u6b21\u8209\u884c\uff09\uff1b\u53e646\u842c\u5143\uff0c\u5206\u4e8823\u91cc\uff08\u6bcf\u91cc2\u842c\u5143\uff0c\u95dc\u5dba\u91cc\u9664\u5916\uff09\u8fa6\u7406\u91cc\u6c11\u4ea4\u6d41\u6d3b\u52d5\u3002\u52de\u5de5\u5c40\uff1a1.\u5747\u4e3b\u52d5\u77ad\u89e3\u52de\u5de5\u722d\u8b70\u4e4b\u8a34\u6c42\uff0c\u4e26\u5354\u52a9\u52de\u5de5\u4f9d\u64da\u52de\u52d5\u76f8\u95dc\u6cd5\u4ee4\u4e3b\u5f35\u5176\u6b0a\u76ca\u30022.\u5ec1\u6240\u4fbf\u5668\u7b49\u885b\u751f\u76f8\u95dc\u8a2d\u5099\uff1a\u696d\u5df2\u65bc103\u5e742\u6708\u5e95\u524d\u5b8c\u6210\u5ec1\u6240\u901a\u98a8\u7dad\u4fee\u7b49\u6539\u5584\u4f5c\u696d\u30023.\u6c23\u51b7\u5f0f\u51b7\u6c23\u6a5f\u3001\u5c0f\u578b\u9001\u98a8\u6a5f\u3001\u51b0\u6c34\u7ba1\u8def\u4fdd\u6ebc\uff1a\u696d\u5df2\u65bc2\u6708\u5e95\u524d\u696d\u5df2\u5148\u5b8c\u6210\u7a7a\u8abf\u8a2d\u5099\u8a2d\u8a08\u3001\u76e3\u9020\u4e4b\u5408\u7d04\uff0c\u523b\u6b63\u53c3\u8003\u76e3\u9020\u5ee0\u5546\u6240\u63d0\u8a2d\u8a08\u5716\u8aaa\u3001\u5de5\u7a0b\u9810\u7b97\u66f8\u3001\u5de5\u7a0b\u6a19\u55ae\u7b49\u8cc7\u6599\u8fa6\u7406\u63a1\u8cfc\u767c\u5305\u4f5c\u696d\uff0c\u9810\u8a08\u65bc3\u6708\u5e95\u524d\u5b8c\u6210\u767c\u5305\u4f5c\u696d\u3002\u6587\u5316\u5c40\uff1a1.\u85dd\u6587\u666e\u67e5\u660e\u7d30\u53ca\u6210\u679c\u696d\u65bc\u7b2c\u4e00\u5c46\u7b2c\u516d\u6b21\u5b9a\u671f\u5927\u6703\u524d\u9001\u6703\u5be9\u8b70\u30022.\u70ba\u914d\u5408\u5e02\u5e9c\u4f4e\u78b3\u653f\u7b56\uff0cGroupe F\u6f14\u51fa\u4e26\u7121\u7159\u706b\u65bd\u653e\uff0c\u6539\u4ee5\u9ad8\u7a7a\u7279\u6280\u3001\u5149\u5f71\u7279\u6548\u7d50\u5408\u97f3\u6a02\u3001\u821e\u8e48\u3001\u6232\u5287\u4e4b\u6f14\u51fa\uff0c\u5177\u74b0\u4fdd\u610f\u8b58\u3001\u54f2\u5b78\u601d\u7dad\u4e4b\u4f5c\u54c1\uff0c\u5169\u65e5\u5171\u5438\u5f153\u842c\u4eba\u6b21\u5230\u5834\u89c0\u8cde\u30023.\u4f9d\u64da\u9810\u7b97\u8ffd\u52a0\u6e1b\u76f8\u95dc\u8fa6\u6cd5\uff0c\u81fa\u5357\u5e02\u7f8e\u8853\u9928\u8208\u8a2d\u5de5\u7a0b\u5c08\u6848\u7ba1\u7406\u8a08\u756b\u4e0d\u7b26\u5408\u8fa6\u7406\u8ffd\u52a0\u6e1b\u8981\u4ef6\uff0c\u6545\u7121\u6cd5\u8fa6\u7406\u8ffd\u52a0\u3002\u6559\u80b2\u5c40\uff1a1.\u91dd\u5c0d\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100010-16\u9801\uff0c\u52a0\u73ed\u8cbb-\u8fa6\u7406\u696d\u52d9\u6240\u9700\u52a0\u73ed\u8cbb\u9810\u7b97\u6578\u65b0\u81fa\u5e6325\u842c\u5143\uff0c\u6559\u80b2\u5c40103\u5e74\u5ea6\u6574\u9ad4\u52a0\u73ed\u8cbb\u4e0d\u5f97\u8d85\u904e102\u5e74\u5ea6\u52a0\u73ed\u8cbb\u7e3d\u984d\u4e00\u6848\uff0c\u5df2\u65bc103\u5e741\u670814\u65e5\u51fd\u5fa9\u8b70\u6703\u4f9d\u6c7a\u8b70\u5167\u5bb9\u8fa6\u7406\u30022.\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u767c\u5c55\u57fa\u91d100013-12\u9801(\u81fa\u5357\u5e02\u9ad4\u80b2\u8655)\uff0c\u6350\u52a9\u79c1\u6821\u53ca\u5718\u9ad4-\u88dc\u52a9\u5404\u9805\u570b\u969b\u53ca\u5168\u570b\u6027\u9326\u6a19\u8cfd\u6d3b\u52d5\u7d93\u8cbb\uff08\u542b\u9435\u4eba\u4e09\u9805\u3001\u81ea\u7531\u8eca\u74b0\u53f0\u8cfd\u53ca\u83ef\u5b97\u76c3\u6392\u7403\u8cfd\u7b49\uff09\u9810\u7b97\u6578\u65b0\u81fa\u5e63725\u842c\u5143\uff0c\u300c\u9435\u4eba\u4e09\u9805\u300d\u4e0d\u5f97\u5728\u89c0\u5915\u5e73\u81fa\u7b49\u5371\u96aa\u6d77\u57df\u8209\u8fa6\uff0c\u5426\u5247\u7d93\u8cbb\u4e0d\u5f97\u52d5\u652f\u3002\u672c\u5e9c\u9ad4\u80b2\u8655\u5df2\u7a4d\u6975\u5c0b\u627e\u9069\u7576\u5834\u5730\uff0c\u6539\u5728\u70cf\u5c71\u982d\u6c34\u5eab\u8209\u8fa6\u30023.\u793e\u5718\u6cd5\u4eba\u81fa\u5357\u5e02\u6559\u5e2b\u6703\u53ca\u81fa\u5357\u5e02\u6559\u80b2\u7522\u696d\u5de5\u6703\u7684\u6559\u5e2b\u6e1b\u6388\u8ab2\u9418\u9ede\u8cbb\u4e0d\u4e88\u88dc\u52a9\u3002\u672c\u6848\u5df2\u65bc103\u5e741\u670820\u65e5\u51fd\u5fa9\u8b70\u6703\uff1a(1)\u672c\u5e02\u6240\u5c6c\u5404\u7d1a\u5b78\u6821\u53c3\u8207\u793e\u5718\u6cd5\u4eba\u81fa\u5357\u5e02\u6559\u5e2b\u6703\u53ca\u81fa\u5357\u5e02\u6559\u80b2\u7522\u696d\u5de5\u6703\u7684\u6559\u5e2b8000\u4eba\uff0c\u7686\u4f9d\u300c\u672c\u5e02\u6559\u5e2b\u66a8\u517c\u4efb\u884c\u653f\u4eba\u54e1\u6bcf\u9031\u6388\u8ab2\u7bc0\u6578\u3001\u5b78\u751f\u6bcf\u9031\u5b78\u7fd2\u7bc0\u6578\u88dc\u5145\u898f\u5b9a\u300d\u6392\u8ab2\uff0c\u60df\u672c\u5e9c\u6559\u80b2\u5c40\u53e6\u6709\u6838\u5b9a\u6e1b\u6388\u8ab2\u7bc0\u6578\u8005\u4e0d\u5728\u6b64\u9650\u3002(2)\u6709\u95dc\u6559\u80b2\u7522\u696d\u5de5\u6703\u6703\u52d9\u516c\u5047\u76f8\u95dc\u554f\u984c\uff0c\u70ba\u5168\u570b\u6027\u4e00\u81f4\u6027\u4e4b\u554f\u984c\uff0c\u672c\u5e9c\u5df2\u65bc102\u5e7411\u670829\u65e5\u53d7\u76e3\u5bdf\u9662\u7d04\u8a62\uff0c\u5c07\u4fdf\u76e3\u5bdf\u9662\u8abf\u67e5\u7d50\u679c\u4f86\u51fd\u5f8c\uff0c\u8207\u5168\u570b\u5404\u7e23\u5e02\u4f5c\u4e00\u81f4\u6027\u901a\u76e4\u6aa2\u8a0e\uff0c\u4e26\u5df2\u5c07\u6b64\u8b70\u984c\u7d0d\u5165\u8207\u6559\u80b2\u7522\u696d\u5de5\u6703\u5718\u9ad4\u5354\u7d04\u5354\u5546\u8b70\u7a0b\u3002\u70ba\u8b93\u672c\u5e9c\u6240\u5c6c\u5404\u7d1a\u5b78\u6821\u7684\u8ab2\u52d9\u5b89\u6392\u5b8c\u6574\u6027\u53ca\u5b78\u751f\u8ab2\u7a0b\u53d7\u6559\u6b0a\u7684\u5ef6\u7e8c\u6027\uff0c\u64ec\u7dad\u6301\u73fe\u6709\u6e1b\u6388\u8ab2\u65b9\u5f0f\u4e0d\u505a\u7570\u52d5\uff0c103\u5b78\u5e74(103\u5e748\u67081\u65e5)\u518d\u505a\u901a\u76e4\u6aa2\u8a0e\u30024.\u81fa\u5357\u5e02\u5730\u65b9\u6559\u80b2\u57fa\u91d1(P00010-20\u9801\u81f3P00010-21  \u9801) 103\u5e74\u5ea6\u6559\u80b2\u5c40-\u570b\u6c11\u5c0f\u5b78\u6559\u80b2\uff0d\u570b\u6c11\u5c0f\u5b78\u6559\u80b2\u884c\u653f\u53ca\u7763\u5c0e\uff0c\u670d\u52d9\u8cbb\u7528\u9805\u4e0b\u6240\u5c6c\u8cc7\u8a0a\u4e2d\u5fc3\u71df\u904b\u6709\u95dc\u9810\u7b97(\u7d30\u76ee\u5982\u4e0b)\u5171\u8a08\u65b0\u81fa\u5e63199\u842c6,000\u5143\uff0c\u6c11\u570b107\u5e74\u4ee5\u5f8c\u501f\u8abf\u8001\u5e2b\u61c9\u5168\u90e8\u6b78\u5efa\u3002\u7d93\u6fdf\u767c\u5c55\u5c40\uff1a\u5df2\u9075\u7167\u6c7a\u8b70\u4e8b\u9805\u8fa6\u7406\u3002\u885b\u751f\u5c40\uff1a\u5236\u5b9a\u6392\u5bcc\u689d\u6b3e\uff0c\u76ee\u524d\u6b63\u6536\u96c6\u5404\u7e23\u5dff\u8cc7\u6599\uff0c\u7814\u8b70\u4e2d\u3002\u74b0\u5883\u4fdd\u8b77\u5c40\uff1a1.\u696d\u4ee5102\u5e7412\u67084\u65e5\u5e9c\u74b0\u6c34\u5b57\u7b2c1021014024\u865f\u51fd\u51fd\u9001\u8b70\u6703\u30022.\u672c\u6848\u5df2\u65bc102\u5e7412\u670820\u65e5\u65bc\u5e02\u8b70\u67031\u6a13\u7c21\u5831\u5ba4\u9032\u884c\u571f\u58e4\u53ca\u5730\u4e0b\u6c34\u59d4\u8fa6\u5de5\u4f5c\u5c08\u6848\u5831\u544a\u30023.\u672c\u5e9c\u74b0\u4fdd\u5c40\u696d\u65bc102\u5e7411\u670814\u65e5\u6703\u540c\u5510\u8b70\u54e1\u78a7\u5a25\u53ca\u5c45\u6c11\u4ee3\u8868\u6797\u5409\u9032\u81f3\u4e2d\u77f3\u5316\u5b89\u9806\u5ee0\u55ae\u4e00\u690d\u88ab\u5340\u5408\u683c\u571f\u66ab\u5b58\u5340\u9032\u884c\u73fe\u52d8\uff0c\u5408\u683c\u571f\u4ee5\u4e0d\u900f\u6c34\u5e06\u5e03\u53ca\u9632\u5875\u9ed1\u7db2\u9032\u884c\u96d9\u5c64\u8986\u84cb\uff0c\u4e26\u589e\u8a2d\u7051\u6c34\u88dd\u7f6e\u4ee5\u52a0\u5f37\u63da\u5875\u9632\u5236\u6210\u6548\u3002\u59d4\u8a17\u76e3\u7763\u55ae\u4f4d\u99d0\u5834\u4eba\u54e1\u4e26\u5168\u7a0b\u76e3\u7763\u5408\u683c\u571f\u79fb\u904b\u53ca\u66ab\u5b58\u4f5c\u696d\uff0c\u82e5\u767c\u73fe\u63da\u5875\u7b49\u4e8c\u6b21\u6c61\u67d3\u60c5\u4e8b\u5247\u7acb\u5373\u8981\u6c42\u505c\u5de5\uff0c\u4e14\u61c9\u65bc\u5b8c\u6210\u6539\u5584\u5f8c\u59cb\u53ef\u6062\u5fa9\u4f5c\u696d\u30024.\u5c07\u914d\u5408\u8abf\u67e5\u55ae\u4f4d\u5099\u59a5\u76f8\u95dc\u63a1\u8cfc\u6848\u4ef6\u8cc7\u6599\u30025.\u5df2\u65bc102\u5e7411\u6708\u73fe\u5834\u63d0\u9001\u59d4\u5916\u64cd\u4f5c\u71df\u904b\u7ba1\u7406\u8cc7\u6599\u81f3\u8b70\u6703\u4f9b\u8b70\u54e1\u5be9\u8996\u5b8c\u7562\u3002\u8b66\u5bdf\u5c40\uff1a1.\u6b72\u5165\u522a\u6e1b3\u5104\u5143(\u4ea4\u901a\u7f70\u9370)\u90e8\u5206\uff0c\u5df2\u4fee\u6b63\u4e26\u5b8c\u6210\u6cd5\u5b9a\u9810\u7b97\u66f8\u5370\u88fd\u30022.102\u5e74\u5ea6\u6cbb\u5b89\u76e3\u8996\u9304\u5f71\u7cfb\u7d71\u5efa\u7f6e\u6848\u9810\u7b97\u7de8\u5217\u65b0\u81fa\u5e63(\u4ee5\u4e0b\u540c)1\u51045700\u842c\u5143\uff0c\u7b2c\u4e00\u6b21\u767c\u5305\u6c7a\u6a19\u91d1\u984d1\u51043650\u5143\uff0c\u99182050\u842c\u5143\u8fa6\u7406\u5f8c\u7e8c\u64f4\u5145\u63a1\u8cfc\uff0c\u6c7a\u6a19\u91d1\u984d1199\u842c8100\u5143\uff0c\u7e3d\u8a08\u6c7a\u6a19\u91d1\u984d1\u51044849\u842c8100\u5143\u3002\u6c34\u5229\u5c40\uff1a\u5c07\u914d\u5408\u8fa6\u7406\u5c08\u6848\u63a7\u7ba1\u3002\u5730\u653f\u5c40\uff1a\u5c07\u53ec\u96c6\u76f8\u95dc\u55ae\u4f4d\u7814\u5546\u8fa6\u7406\u671f\u7a0b\u53ca\u5de5\u4f5c\u9805\u76ee\uff0c\u4e26\u7ba1\u63a7\u9032\u5ea6\uff0c\u52d9\u5fc5\u905490%\u57f7\u884c\u7387\u3002\u5de5\u52d9\u5c40\uff1a\u5df2\u65bc103.02.25\u5c08\u6848\u7c3d\u8fa6\uff0c\u7d93\u79d8\u66f8\u9577\u88c1\u793a\u4ea4\u81fa\u5357\u516c\u5712\u767e\u5e74\u518d\u751f\u8a08\u756b\u5c08\u6848\u5c0f\u7d44\u8a0e\u8ad6\u7814\u8b70\u3002","\u6c7a\u8b70\u65e5\u671f":"2013\/12\/6","\u767c\u6587\u65e5\u671f":"2013\/12\/12","\u767c\u6587\u5b57\u865f":"\u5357\u8b70\u8ca1\u7d93\u5b57\u7b2c1020011161\u865f","\u8b70\u6703\u9644\u4ef6\u4e00":"\u4e0b\u8f09&nbsp;\u4e0b\u8f09&nbsp;"}