              		<td align="left" valign="top" background="/images/newdata/images_2007/form_x02.gif">
              			<table width="98%" border="0" align="center" cellpadding="2" cellspacing="5">
                			<tbody>
                  			<tr bgcolor="#f2f2f2">
                    			<td align="right">                     
									<img height="18" src="/images/newdata/images_2007/mail.gif" width="19" align="absmiddle" />
									<a class="story_function" href="/ndapp/FwdArticle?id=7704927&udndbid=udnfree&searchString=pP2vRSuk6bTBPj0yMDEwMDEwMSuk6bTBPD0yMDE0MTIyNSuz%2BKdPPcFwpliz%2BHy4Z8DZpOmz%2BHzBcKZYsd%2Bz%2BHxVcGFwZXI%3D" target="_blank">轉寄</a> 
									<img height="18" src="/images/newdata/images_2007/print.gif" width="19" align="absmiddle" />
									<a class="story_function" href="javascript:openWindow('/ndapp/Print?id=7704927&udndbid=udnfree&date=2014-10-11','check','scrollbars,resizable=yes,width=640,height=480')">友善列印</a>
