          	<table width="100%" border="0" cellpadding="10" cellspacing="0">
            	<tr>
              		<td align="left" valign="top" background="/images/newdata/images_2007/form_x02.gif">
              			<table width="98%" border="0" align="center" cellpadding="2" cellspacing="5">
                			<tbody>
                  			<tr bgcolor="#f2f2f2">
                    			<td align="right">                     
									<img height="18" src="/images/newdata/images_2007/mail.gif" width="19" align="absmiddle" />
									<a class="story_function" href="/ndapp/FwdArticle?id=5304720&udndbid=udnfree&searchString=t6i56qzuK6TptME%2BPTIwMTAwMTAxK6TptME8PTIwMTQxMjI1K7P4p089wXCmWLP4fLhnwNmk6bP4fMFwplix37P4fFVwYXBlcg%3D%3D" target="_blank">轉寄</a> 
									<img height="18" src="/images/newdata/images_2007/print.gif" width="19" align="absmiddle" />
									<a class="story_function" href="javascript:openWindow('/ndapp/Print?id=5304720&udndbid=udnfree&date=2010-03-21','check','scrollbars,resizable=yes,width=640,height=480')">友善列印</a>
