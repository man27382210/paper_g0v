              			<table width="98%" border="0" align="center" cellpadding="2" cellspacing="5">
                			<tbody>
                  			<tr bgcolor="#f2f2f2">
                    			<td align="right">                     
									<img height="18" src="/images/newdata/images_2007/mail.gif" width="19" align="absmiddle" />
									<a class="story_function" href="/ndapp/FwdArticle?id=5410239&udndbid=udnfree&searchString=vEav1a7bqtorpOm0wT49MjAxMDAxMDErpOm0wTw9MjAxNDEyMjUrs%2FinTz3BcKZYs%2Fh8uGfA2aTps%2Fh8wXCmWLHfs%2Fh8VXBhcGVy" target="_blank">轉寄</a> 
