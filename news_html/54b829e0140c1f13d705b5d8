              			<table width="98%" border="0" align="center" cellpadding="2" cellspacing="5">
                			<tbody>
                  			<tr bgcolor="#f2f2f2">
                    			<td align="right">                     
									<img height="18" src="/images/newdata/images_2007/mail.gif" width="19" align="absmiddle" />
									<a class="story_function" href="/ndapp/FwdArticle?id=7536579&udndbid=udnfree&searchString=pP2lQLDtK6TptME%2BPTIwMTAwMTAxK6TptME8PTIwMTQxMjI1K7P4p089wXCmWLP4fLhnwNmk6bP4fMFwplix37P4fFVwYXBlcg%3D%3D" target="_blank">轉寄</a> 
									<img height="18" src="/images/newdata/images_2007/print.gif" width="19" align="absmiddle" />
									<a class="story_function" href="javascript:openWindow('/ndapp/Print?id=7536579&udndbid=udnfree&date=2014-06-02','check','scrollbars,resizable=yes,width=640,height=480')">友善列印</a>
